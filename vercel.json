{
  "version": 2,
  "buildCommand": "echo 'No build needed'",
  "outputDirectory": ".",
  "builds": [
    {
      "src": "api/ai/coze.py",
      "use": "@vercel/python"
    },
    {
      "src": "api/ai/deepseek.py",
      "use": "@vercel/python"
    },
    {
      "src": "api/auth/login.py",
      "use": "@vercel/python"
    },
    {
      "src": "api/auth/register.py",
      "use": "@vercel/python"
    },
    {
      "src": "api/movies/list.py",
      "use": "@vercel/python"
    },
    {
      "src": "api/news/list.py",
      "use": "@vercel/python"
    },
    {
      "src": "api/dictionary/history.py",
      "use": "@vercel/python"
    },
    {
      "src": "api/config.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/public/(.*)",
      "dest": "/public/$1"
    },
    {
      "src": "/(.*\\.(js|css|json|png|jpg|jpeg|gif|ico|svg|woff|woff2|ttf|eot))",
      "dest": "/$1"
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}


